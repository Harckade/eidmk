{
  "name": "Entra ID Master Key (EIDMK)",
  "version": "1.0",
  "description": "Bypass Microsoft Entra ID (prev. Azure Active Directory) restrictions and do everything you are allowed to do on CLI but on the UI.",
  "manifest_version": 3,
  "permissions": ["storage", "tabs", "debugger"],
  "host_permissions": ["*://graph.microsoft.com/*", "*://main.iam.ad.ext.azure.com/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
		"default_icon": "icon.png",
		"default_title": "EIDMK",
		"default_popup": "popup.html"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
